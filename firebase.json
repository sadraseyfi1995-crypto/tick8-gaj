{
    "hosting": {
        "public": "dist/tick8-gaj",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [
            {
                "source": "**",
                "destination": "/index.html"
            }
        ],
        "headers": [
            {
                "source": "**",
                "headers": [
                    {
                        "key": "Cross-Origin-Opener-Policy",
                        "value": "unsafe-none"
                    },
                    {
                        "key": "Cross-Origin-Embedder-Policy",
                        "value": "unsafe-none"
                    }
                ]
            }
        ]
    }
}